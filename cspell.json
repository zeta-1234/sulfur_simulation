{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en",
    "words": [],
    "ignoreWords": [
        "Phonopy",
        "Wannier",
        "pytest",
        "pyright",
        "pydata",
        "pylint",
        "pydocstyle",
        "numpy",
        "isort",
        "ndarray",
        "xlabel",
        "ylabel",
        "pyproject",
        "scipy",
        "neighbour",
        "maxfev",
        "blit",
        "xlim",
        "ylim",
        "xticks",
        "yticks",
        "tqdm",
        "trange",
        "timesteps",
        "ifft",
        "brentq",
        "waals",
        "zorder",
        "xticklabels",
        "pickler",
        "savez",
        "unpickler",
        "normalise",
        "normalised",
    ],
    "flagWords": [
        "hte",
        "teh"
    ],
    "allowCompoundWords": true,
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        "*.snap",
        "node_modules",
        "__snapshots__",
        "package-lock.json",
        "coverage/**",
        "dist/**",
        "dist.*/**",
        "temp/**",
        "CHANGELOG.md",
        "package.json",
        ".cspell.json",
        "cspell.json",
        ".vscode/**",
        ".devcontainer/**",
        ".github/**",
        ".pytest_cache/**",
        ".ruff_cache/**",
        ".venv/**",
        ".vscode/**",
        "build/**",
        "docs/**",
        "*.egg-info/**"
    ],
    "noConfigSearch": true
}